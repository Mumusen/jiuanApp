header {
  width: 100%;
  height: 1.70667rem;
  line-height: 1.70667rem;
  text-align: center;
  background-color: #ea1f38;
  color: #fff;
  font-size: 0.48rem;
  position: fixed;
  z-index: 99999998;
  top: 0;
}

section {
  padding-top: 1.70667rem;
}

.statusBar {
  height: 0.58667rem;
}

.content {
