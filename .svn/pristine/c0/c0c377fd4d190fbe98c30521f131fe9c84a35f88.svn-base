}

.title,
.tit {
  width: 9.2rem;
  margin: 0 auto;
}

.por .title {
  height: 1.2rem;
  line-height: 1.2rem;
  background-color: #fff;
  color: #999;
  font-size: 0.42667rem;
  position: absolute;
  top: 1.73333rem;
  left: 0.4rem;
