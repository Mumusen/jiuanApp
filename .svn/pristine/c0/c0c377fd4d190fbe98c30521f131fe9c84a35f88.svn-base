  position: relative;
  text-align: left;
  text-indent: 0.53333rem;
}

.list li .li span:nth-child(1) i {
  position: absolute;
  height: 0.26667rem;
  line-height: 0.26667rem;
  top: 1.06667rem;
  left: 0;
  font-size: 0.26667rem;
  color: #999;
}

.list li .li span:nth-child(2) {
  color: #39c922;
  width: 2.26667rem;
  padding-right: 1.33333rem;
}

.list li .li u {
  min-width: 1.33333rem;
  height: 0.66667rem;
  line-height: 0.66667rem;
  padding: 0 0.13333rem;
  font-family: 'Arial';
  display: block;
  background-color: #39c922;
  border-radius: 0.08rem;
  color: #fff;
  font-size: 0.37333rem;
  margin: 0.4rem 0.4rem 0 0;
}

.list .hot-txt .li span:nth-child(2) {
  color: #ea1f38;
}

.list .hot-txt .li span:nth-child(3) u {
  background-color: #ea1f38;
}

.list .theme,
.list .list-themeli {
  position: relative;
  text-align: left;
}

.list .theme .li span:nth-child(1) b,
.list .list-themeli .li span:nth-child(1) b {
  font-size: 0.24rem;
  color: #fff;
  background-color: #ea1f38;
  height: 0.34667rem;
  line-height: 0.34667rem;
  text-align: center;
  border-radius: 0.08rem;
  top: 0.6rem;
  text-indent: 0;
  padding: 0 0.05333rem;
  margin-left: 0.26667rem;
  position: absolute;
}

.list .theme .li span:nth-child(2),
.list .list-themeli .li span:nth-child(2) {
