}

.list .theme,
.list .list-themeli {
  height: 1.2rem;
}

.list .theme .li,
.list .list-themeli .li {
  height: 1.13333rem;
  line-height: 1.13333rem;
}

.list .theme .li span:nth-child(1) b,
.list .list-themeli .li span:nth-child(1) b {
  top: 0.37333rem;
}

.list .theme .li u,
.list .list-themeli .li u {
  margin-top: 0.26667rem;
}

.list .theme .btn,
.list .list-themeli .btn {
