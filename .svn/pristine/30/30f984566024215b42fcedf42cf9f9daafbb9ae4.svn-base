.box {
  display: -webkit-box;
  -webkit-box-sizing: border-box;
}

.box > li {
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
}

.min {
  width: 100%;
  height: 3.73333rem;
  background: url(../image/index_banner.png) no-repeat center;
  background-size: cover;
  position: relative;
  margin-bottom: 1.86667rem;
}

.min i {
  width: 0.6rem;
  height: 0.6rem;
  position: absolute;
  background-size: 100%;
  background-position: center;
  top: 0.82667rem;
}

.min .user-btn {
  left: 0.4rem;
  background-image: url(../image/icon_user.png);
  background-size: 100%;
}

.min .search-btn {
  right: 0.4rem;
  background-image: url(../image/icon_search.png);
}

.box {
  position: absolute;
  bottom: -1.6rem;
  width: 92%;
  margin: 0.4rem 0.4rem 0.26667rem;
  background: #fff;
  box-shadow: 0 0.05333rem 0.13333rem rgba(81, 71, 72, 0.28);
  border-radius: 0.13333rem;
}

.box li {
  padding: 0.4rem;
  font-family: 'pinfangMedium';
  position: relative;
}

.box li h3 {
  font-size: 0.37333rem;
  color: #333;
}

.box li h4 {
  font-size: 0.42667rem;
  color: #39c922;
}

.box li p,
.box li span {
  font-family: Arial;
  color: #39c922;
  font-size: 0.26667rem;
}

.box li::before {
  content: "";
  width: 0.02667rem;
  height: 0.8rem;
  background-color: #eee;
  position: absolute;
  right: 0;
  top: 0.66667rem;
}

.box li:nth-child(3):before {
  display: none;
}

.theme {
  padding: 0.26667rem 0.4rem 0 0.4rem;
  border-bottom: 0.26667rem solid #f7f7f7;
}

.theme .title {
  position: relative;
}

.theme .title p {
  font-size: 0.42667rem;
  font-family: 'pinfangRegular';
  color: #bfbfbf;
}

.theme .title p span:nth-child(1) {
  margin-right: 0.53333rem;
}

.theme .title p .active {
  font-family: 'pinfangBold';
  color: #ea1f38;
}

.theme .title #tabBox .active:nth-child(2) {
  color: #39c922;
}

.theme .title .more-btn {
  width: 0.53333rem;
  height: 0.53333rem;
  position: absolute;
  right: 0;
  top: 0;
}

.theme .title .more-btn::before {
  content: "";
  float: right;
  margin-top: 0.13333rem;
  width: 0.2rem;
  height: 0.37333rem;
  background: url(../image/icon_more.png) no-repeat center;
  background-size: cover;
}

.rise {
  display: none;
}

.rise li {
  border-bottom: 0.02667rem solid #eee;
  overflow: hidden;
  height: 1.2rem;
  padding: 0.4rem 0 0.53333rem 0;
}

.rise li .left {
  float: left;
}

.rise li .left h2 {
  font-size: 0.42667rem;
  font-family: 'pinfangMedium';
}

.rise li .left .txt p {
  float: left;
  font-size: 0.37333rem;
  font-family: 'Arial';
  padding-top: 0.13333rem;
  height: 0.48rem;
  line-height: 0.48rem;
  color: #ea1f38;
}

.rise li .left .txt p i {
  width: 0.74667rem;
  height: 0.34667rem;
  float: left;
  background: url(../image/icon_first_up.png);
  background-size: 100%;
  margin: 0.05333rem 0.13333rem;
  border: 0.02667rem solid #ea1f38;
  border-radius: 0.08rem;
}

.rise li .left .txt p span {
  font-family: 'pinfangRegular';
  color: #666;
}

.rise li .left .txt p span u {
  font-family: 'Arial';
  color: #ea1f38;
  padding-left: 0.26667rem;
}

.rise li .left .txt p:nth-child(1) {
  width: 2.13333rem;
}

.rise li .left .txt .green {
  color: #39c922;
}

.rise li .left .txt .green-icon i {
  border-color: #39c922;
  background: url(../image/icon_first_down.png) no-repeat;
  background-size: cover;
}

.rise li .left .txt .green-icon u {
  color: #39c922;
}

.rise li .right {
  float: right;
}

.rise li .right .long,
.rise li .right .follow {
  float: left;
  height: 1.2rem;
}

.rise li .right .long img,
.rise li .right .follow img {
  width: 100%;
  margin: 0.32rem 0;
}

.rise li .right .long {
  width: 1.36rem;
  padding-right: 0.53333rem;
}

.rise li .right .follow {
  margin-right: 0.4rem;
}

.rise li .right .follow::before {
  content: "";
  display: block;
  width: 1.86667rem;
  height: 0.8rem;
  background: url(../image/icon_follow_ok.png) no-repeat center;
  background-size: cover;
  margin: 0.32rem 0;
}

.rise li .right .follow-no::before {
  background-image: url(../image/icon_follow_no.png);
}

.rise li:last-of-type {
  border: none;
}

.info {
  padding-top: 0.4rem;
}

.info .title {
  font-family: 'pinfangBold';
  font-size: 0.42667rem;
  text-align: center;
  color: #999;
  position: relative;
}

.info .title::before,
.info .title::after {
  content: "";
  position: absolute;
  width: 0.4rem;
  height: 0.05333rem;
  background-color: #eee;
  top: 0.24rem;
}

.info .title::before {
  left: 3.28rem;
}

.info .title::after {
  right: 3.28rem;
}

.info .time-date {
  font-size: 0.42667rem;
  font-family: 'Arial';
  height: 0.93333rem;
  line-height: 2.5;
  text-indent: 1.2rem;
  position: relative;
  background-color: #eee;
  margin: 0.26667rem 0;
}

.info .time-date::before {
  content: "";
  width: 0.6rem;
  height: 0.6rem;
  background: url(../image/date_icon.png) no-repeat center;
  background-size: 100%;
  position: absolute;
  top: 50%;
  left: 0.4rem;
  margin-top: -0.3rem;
}

.info .mian {
  display: -webkit-box;
  padding: 0 0.4rem;
}

.info .mian .day {
  width: 1.46667rem;
  float: left;
  position: relative;
}

.info .mian .day span {
  width: 0.93333rem;
  height: 0.34667rem;
  line-height: 0.34667rem;
  border-radius: 0.17333rem;
  z-index: 9;
  position: absolute;
  top: 0.4rem;
  font-size: 0.29333rem;
  text-align: center;
  background-color: #fff;
  border: 1px solid #999;
  color: #999;
  font-family: "Arial";
}

.info .mian .day i {
  position: absolute;
  width: 0.02667rem;
  height: 100%;
  left: 0.46667rem;
  background-color: #eee;
  z-index: 1;
}

.info .mian .txt {
  -webkit-box-flex: 3.5;
}

.info .mian .txt h2 {
  font-size: 0.42667rem;
  font-family: "pinfangMedium";
  color: #333;
  margin: 0.26667rem 0;
  height: 0.4rem;
  position: relative;
}

.info .mian .txt h2 u {
  font-size: 0.29333rem;
  color: #fff;
  background-color: #ea1f38;
  width: 0.90667rem;
  height: 0.48rem;
  line-height: 0.48rem;
  text-align: center;
  border-radius: 0.08rem;
  top: 0.08rem;
  position: absolute;
  margin-left: 0.13333rem;
}

.info .mian .txt p {
  font-size: 0.37333rem;
  color: #999;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-bottom: 0.13333rem;
}

.info .mian .txt p span {
  font-size: 0.32rem;
  font-family: "pinfangMedium";
  color: #39c922;
}

.info .mian .txt p span u {
  font-family: "Arial";
  margin-left: 0.26667rem;
}

.info .mian .txt p span:nth-child(2) {
  margin-left: 1.33333rem;
}

.info .mian .txt p b {
  color: #514748;
}

.info .news-up {
  padding: 0.4rem 0.4rem 0 1rem;
  position: relative;
}

.info .news-up .time-date-up {
  font-size: 0.42667rem;
  font-family: 'Arial';
  color: #999;
  font-weight: 600;
}

.info .news-up .txt h2 {
  margin-top: 0.13333rem;
}

.info .news-up::before {
  content: "";
  position: absolute;
  left: 0.4rem;
  top: 0.53333rem;
  width: 0.18667rem;
  height: 0.18667rem;
  background-color: #ea1f38;
  border-radius: 50%;
}

.frame-tit span:nth-child(1) {
  text-align: left;
  -webkit-box-align: left;
  text-indent: -1.06667rem;
}

.btn-more-news {
  font-family: 'pinfangBold';
  text-align: center;
  display: block;
  font-size: 0.34667rem;
  color: #999;
  padding: 0.4rem 0;
}

.liftBtn {
  position: relative;
}

.liftBtn:before {
  content: "";
  position: absolute;
  width: 0;
  height: 0;
  right: 0.53333rem;
  top: 0.13333rem;
  border-left: 0.13333rem solid transparent;
  border-right: 0.13333rem solid transparent;
  border-bottom: 0.10667rem solid #dedede;
}

.liftBtn::after {
  content: "";
  position: absolute;
  width: 0;
  height: 0;
  right: 0.53333rem;
  top: 0.26667rem;
  border-left: 0.13333rem solid transparent;
  border-right: 0.13333rem solid transparent;
  border-top: 0.10667rem solid #dedede;
}

.up::before {
  border-bottom-color: #ea1f38;
}

.down::after {
  border-top-color: #ea1f38;
}

.choice-tit span:nth-child(1) {
  -webkit-box-flex: 2;
}

.choice-tit span:nth-child(2) {
  -webkit-box-flex: 3;
}

.choice-tit span:nth-child(3) {
  -webkit-box-flex: 3;
}

.tab-list .list-choice {
  display: block;
}

.tab-list {
  position: relative;
}

.btn-all {
  position: absolute;
  top: -3.49333rem;
  left: 0.4rem;
  font-size: 0.42667rem;
  color: #fff;
  z-index: 99999999;
}

.list-choice-edit {
  width: 100%;
  height: 13.06667rem;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  padding-bottom: 0.26667rem;
  overflow-x: auto;
  padding-bottom: 2.50667rem;
}

.list-choice li {
  height: 0.8rem;
  line-height: 0.8rem;
  font-size: 0.42667rem;
  padding: 0.26667rem 0.4rem;
}

.list-choice li i,
.list-choice li span,
.list-choice li input {
  float: left;
}

.list-choice li input {
  width: 0.64rem;
  height: 0.64rem;
  margin: 0.08rem 0.33333rem 0.08rem 0;
  background: url(../image/checkbox_leon.png) no-repeat 0 0;
  background-size: 98%;
  border: 0;
}

.list-choice li input:checked {
  background-position: 0 100%;
}

.list-choice li i,
.list-choice li b {
  width: 0.66667rem;
  height: 0.66667rem;
  background: url(../image/choice_icon_leon.png) no-repeat center;
  background-size: 0.66667rem;
  margin: 0.06667rem 0;
}

.list-choice li .icon-up {
  background-position: 0 0;
}

.list-choice li .icon-drag {
  float: left;
  background-position: 0 100%;
  margin-left: 1.6rem;
}

.list-choice li span {
  width: 4.4rem;
  line-height: 0.53333rem;
  position: relative;
}

.list-choice li span u {
  display: block;
  font-size: 0.29333rem;
  color: #999;
  position: absolute;
  top: 0.4rem;
}

.list-choice li span b {
  font-size: 0.24rem;
  color: #fff;
  background: #ea1f38;
  height: 0.34667rem;
  line-height: 0.34667rem;
  padding: 0 0.05333rem;
  text-align: center;
  border-radius: 0.08rem;
  margin-left: 0.13333rem;
}

.list-choice li .shares {
  line-height: 0.8rem;
}

.list-choice-theme li span {
  line-height: 0.8rem;
}

.lyaer-choices {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 2.50667rem;
  background: -webkit-linear-gradient(rgba(255, 255, 255, 0.6), #fff);
}

.lyaer-choices button {
  width: 5.33333rem;
  height: 1.17333rem;
  line-height: 1.17333rem;
  background-color: #d0ba89;
  border-radius: 0.58667rem;
  color: #fff;
  font-size: 0.42667rem;
  display: block;
  margin: 0.66667rem auto;
  box-shadow: 0 0.13333rem 0.26667rem rgba(211, 188, 137, 0.8);
}

.layer-del {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.4);
}

.layer-del .layer-del-box {
  background-color: #fff;
  width: 5.33333rem;
  padding: 0.66667rem 0.4rem;
  border-radius: 0.26667rem;
  position: absolute;
  left: 50%;
  margin-left: -3.06667rem;
  top: 40%;
  text-align: center;
}

.layer-del .layer-del-box p {
  font-size: 0.42667rem;
  color: #ea1f38;
}

.layer-del .layer-del-box .btn {
  width: 100%;
  display: -webkit-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  margin-top: 0.53333rem;
}

.layer-del .layer-del-box .btn a {
  display: block;
  width: 2.4rem;
  height: 1.06667rem;
  line-height: 1.06667rem;
  border: 0.02667rem solid #ea1f38;
  color: #ea1f38;
  border-radius: 0.53333rem;
  font-size: 0.42667rem;
}
