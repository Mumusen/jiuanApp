section {
  padding: 0;
}

.vip-app {
  padding-top: 1.70667rem;
}

.header-title {
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
  background-color: #eee;
  height: 0.90667rem;
  line-height: 0.90667rem;
}

.header-title li {
  height: 0.90667rem;
  line-height: 0.90667rem;
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
}

.header-title li a {
  height: 0.90667rem;
  line-height: 0.90667rem;
  font-size: 0.37333rem;
  color: #999;
  box-sizing: border-box;
}

.header-title li .active {
  color: #ea1f38;
  border-bottom: 0.02667rem solid #ea1f38;
}

.vip-min {
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
  padding: 0.4rem;
}

.vip-min-control {
  width: 5.86667rem;
  font-size: 0.37333rem;
}

.vip-min-control .box {
  height: 0.93333rem;
  line-height: 0.93333rem;
  border: 0.02667rem solid #ea1f38;
  border-radius: 0.08rem;
  margin-bottom: 0.26667rem;
  padding: 0 0.26667rem;
  position: relative;
}

.vip-min-control .box input {
  height: 0.4rem;
  line-height: 0.4rem;
}

.vip-min-control div.box-btn-br {
  border-color: #dedede;
}

.vip-min-control button {
  width: 100%;
  height: 0.93333rem;
  background-color: #ea1f38;
  color: #fff;
  border-radius: 0.08rem;
}

.number-btn-box input {
  width: 3.6rem;
  margin: 0.26667rem auto;
  display: block;
  text-align: center;
}

.number-btn-box i {
  position: absolute;
  top: 0;
  width: 1.06667rem;
  height: 0.93333rem;
  background: #ea1f38 url(../image/number_btn_icon.png) no-repeat;
  background-size: cover;
}

.number-btn-box .reduce-icon {
  left: 0;
  background-position: 0 100%;
}

.number-btn-box .puls-icon {
  right: 0;
}

.box-txt {
  margin-bottom: 0.26667rem;
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
}

.box-txt span {
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  -webkit-box-align: inherit;
  text-align: left;
  color: #999;
}

.box-txt span:last-child {
  text-align: right;
}

.box-txt-num input {
  width: 2.66667rem;
}

.box-txt-num span {
  color: #666;
}

.box-txt-num span:last-child {
  color: #dedede;
}

.box-btn {
  position: absolute;
  top: 0;
  left: 0;
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
}

.box-btn span {
  -webkit-box-align: center;
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
}

.box-btn .active {
  border: 0.02667rem solid #ea1f38;
  border-radius: 0.08rem;
}

.search-list {
  width: 100%;
  height: 5.36rem;
  text-align: center;
  position: absolute;
  left: -0.02667rem;
  top: 1.22667rem;
  z-index: 9;
  border: 0.02667rem solid #ea1f38;
  background-color: #fff;
  border-radius: 0.08rem;
  display: none;
}

.search-list li {
  font-size: 0.37333rem;
  height: 0.86667rem;
  line-height: 0.86667rem;
  color: #666;
}

.vip-min-list {
  -webkit-box-flex: 1;
  margin-left: 0.53333rem;
}

.vip-min-list li {
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
  line-height: 0.66667rem;
}

.vip-min-list li span {
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  font-size: 0.29333rem;
  color: #999;
}

.vip-min-list li span:nth-child(2) {
  color: #ea1f38;
}

.sell-list::after {
  content: "";
  display: block;
  width: 80%;
  margin: 0 auto;
  height: 0.26667rem;
  border-top: 0.02667rem solid #eee;
}

.webkit {
  height: 0.90667rem;
  font-size: 0.37333rem;
  border-top: 0.02667rem solid #dedede;
  border-bottom: 0.02667rem solid #dedede;
}

.vip-list {
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
  position: relative;
}

.vip-list li {
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  font-size: 0.37333rem;
  padding: 0.29333rem 0;
  font-family: 'Arial';
}

.vip-list li:nth-child(1) h2 {
  font-family: 'pinfangBold';
}

.vip-list li p:nth-child(2) {
  margin-top: 0.26667rem;
}

.vip-list li:nth-child(1) p:nth-child(2) {
  margin-top: 0.16rem;
}

.vip-list::before {
  position: absolute;
  bottom: 0;
  content: "";
  width: 90%;
  left: 10%;
  margin-left: -5%;
  height: 0.02667rem;
  background-color: #dedede;
}

.vip-list li:nth-child(2) {
  color: #39c922;
}

.vip-list .red {
  color: #ea1f38;
}

.vip-list:last-child::before {
  display: none;
}

.vip-min-sell .box, .vip-min-sell .search-list, .vip-min-sell .active {
  border-color: #2ea6ef;
}

.vip-min-sell i, .vip-min-sell button {
  background-color: #2ea6ef;
}

.vip-warehouse .num {
  color: #ea1f38;
}

.vip-warehouse ul {
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
  padding: 0.53333rem 0.4rem;
}

.vip-warehouse ul li {
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  font-size: 0.37333rem;
}

.vip-warehouse ul li h3 {
  color: #999;
}

.vip-warehouse ul li p {
  font-family: 'Arial';
  padding-top: 0.26667rem;
}

.vip-warehouse ul:nth-child(2) {
  padding-top: 0;
}

.vip-warehouse-green .num {
  color: #39c922;
}

.cancel-boxs {
  padding-top: 1.70667rem;
}

.cancel-boxs p {
  color: #999;
}

.cancel-boxs li:nth-child(4) p:nth-child(1) {
  color: #39c922;
}

.cancel-boxs li:nth-child(4) p.buy {
  color: #ea1f38;
}

.cancel-boxs .num {
  font-family: 'Arial';
  font-weight: 600;
  color: #333;
}

.cancel-boxs li p:nth-child(2) {
  margin-top: 0.08rem;
}

.entrust-box {
  padding: 0 0.4rem;
  font-size: 0.37333rem;
}

.entrust-box ul li {
  height: 1.38667rem;
  line-height: 1.38667rem;
  border-bottom: 0.02667rem solid #eee;
  position: relative;
}

.entrust-box ul li a {
  color: #333;
  position: relative;
}

.entrust-box ul li::before {
  position: absolute;
  right: 0;
  top: 0.50667rem;
  content: "";
  width: 0.2rem;
  height: 0.37333rem;
  background: url(../image/user_icon_right_leon.png) no-repeat center;
  background-size: cover;
}

.entrust-box ul:nth-child(1) {
  margin-bottom: 0.13333rem;
  position: relative;
}

.entrust-box ul:nth-child(1)::before {
  content: "";
  width: 110%;
  height: 0.13333rem;
  background-color: #eee;
  position: absolute;
  bottom: -0.13333rem;
  left: -5%;
}

.time-box {
  padding: 0.4rem 1.33333rem;
  display: -webkit-box;
  width: 100%;
  -webkit-box-sizing: border-box;
}

.time-box .rise, .time-box .end {
  -webkit-box-flex: 1;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  font-size: 0.37333rem;
}

.time-box .rise p, .time-box .end p {
  font-size: 0.42667rem;
  font-family: 'Arial';
  font-weight: 600;
  color: #333;
  padding-top: 0.13333rem;
}

.time-box .rise {
  position: relative;
}

.time-box .rise::before {
  content: "";
  width: 0.02667rem;
  height: 0.8rem;
  background-color: #bfbfbf;
  position: absolute;
  right: 0;
  top: 0.13333rem;
}
